<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
	<mime-type type="application/x-egg">
		<comment>알집 압축 파일</comment>
		<glob pattern="*.egg"/>
		<magic priority="50">
			<match value="EGGA" type="string" offset="0"/>
		</magic>
		<icon name="arkzip-egg"/>
	</mime-type>
	<mime-type type="application/x-alz">
		<comment>알집 압축 파일</comment>
		<glob pattern="*.alz"/>
		<magic priority="50">
			<match value="ALZ" type="string" offset="0"/>
		</magic>
		<icon name="arkzip-alz"/>
	</mime-type>
	<mime-type type="application/x-bh">
		<comment>BlakHole 압축 파일</comment>
		<glob pattern="*.bh"/>
		<icon name="arkzip-bh"/>
	</mime-type>
	<mime-type type="application/x-acb">
		<comment>ACB 압축 파일</comment>
		<glob pattern="*.acb"/>
		<icon name="arkzip-acb"/>
	</mime-type>	
	<mime-type type="application/x-udf">
		<comment>Universal Disk Format 파일</comment>
		<glob pattern="*.udf"/>
		<icon name="arkzip-udf"/>
	</mime-type>	
	<mime-type type="application/x-img">
		<comment>raw disk image file format</comment>
		<glob pattern="*.img"/>
		<icon name="arkzip-img"/>
	</mime-type>
	<mime-type type="application/x-j2j">
		<comment>j2j로 변조된 파일</comment>
		<glob pattern="*.j2j"/>
		<icon name="arkzip-j2j"/>
	</mime-type>	
	<mime-type type="application/x-nsis">
		<comment>Nullsoft Scriptable Install System 파일</comment>
		<glob pattern="*.nsis"/>
		<icon name="arkzip-nsis"/>
	</mime-type>	
</mime-info>